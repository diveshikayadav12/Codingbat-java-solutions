ğŸ“ Program16_StudentInterface.java

interface Student {
    void info();
}

class UG implements Student {
    public void info(){ System.out.println("UG Student"); }
}

class PG implements Student {
    public void info(){ System.out.println("PG Student"); }
}

class Program16_StudentInterface {
    public static void main(String[] args) {
        new UG().info();
        new PG().info();
    }
}


