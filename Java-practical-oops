ğŸ“ Program6_QuadraticEquation.java

class Program6_QuadraticEquation {
    public static void main(String[] args) {
        int a = 1, b = 5, c = 6;
        double d = b*b - 4*a*c;

        if(d > 0) System.out.println("Real Roots");
        else if(d == 0) System.out.println("Equal Roots");
        else System.out.println("Imaginary Roots");
    }
}


