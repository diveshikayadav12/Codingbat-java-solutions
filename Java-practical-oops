ğŸ“ Program13 (Package Program)

ğŸ“ File 1: mypackage/Student.java

package mypackage;

public class Student {
    public void display(){
        System.out.println("Hello from Student package");
    }
}

ğŸ“ File 2: Program13_Package.java

import mypackage.Student;

class Program13_Package {
    public static void main(String[] args) {
        Student s = new Student();
        s.display();
    }
}


