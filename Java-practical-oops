ğŸ“ Program10_Inheritance.java

class Parent {
    int x = 10;
}

class Child extends Parent {
    void show() {
        System.out.println(x);
    }
}

class Program10_Inheritance {
    public static void main(String[] args) {
        Child obj = new Child();
        obj.show();
    }
}


---

ğŸ“ Program11_Polymorphism.java

class Poly {
    int sum(int a, int b){ return a + b; }
    int sum(int a, int b, int c){ return a + b + c; }
}

class Program11_Polymorphism {
    public static void main(String[] args) {
        Poly p = new Poly();
        System.out.println(p.sum(2,3));
        System.out.println(p.sum(2,3,4));
    }
}


---

ğŸ“ Program12_MethodOverriding.java

class A {
    void show(){ System.out.println("Parent"); }
}

class B extends A {
    void show(){ System.out.println("Child"); }
}

class Program12_MethodOverriding {
    public static void main(String[] args) {
        B obj = new B();
        obj.show();
    }
}


