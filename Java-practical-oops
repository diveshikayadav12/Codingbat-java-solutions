ğŸ“ Program7_Fibonacci.java

class Program7_Fibonacci {
    public static void main(String[] args) {
        int n = 10, a = 0, b = 1;

        for(int i = 0; i < n; i++){
            System.out.print(a + " ");
            int c = a + b;
            a = b;
            b = c;
        }
    }
}


---

ğŸ“ Program8_MatrixMultiplication.java

class Program8_MatrixMultiplication {
    public static void main(String[] args) {
        int A[][] = { {1,2,3}, {4,5,6}, {7,8,9} };
        int B[][] = { {1,2,3}, {4,5,6}, {7,8,9} };

        int C[][] = new int[3][3];

        for(int i=0;i<3;i++)
            for(int j=0;j<3;j++)
                for(int k=0;k<3;k++)
                    C[i][j] += A[i][k] * B[k][j];

        for(int[] row : C){
            for(int x : row) System.out.print(x + " ");
            System.out.println();
        }
    }
}


