ğŸ“ Program15_RunnableThread.java

class MyThread implements Runnable {
    public void run() {
        System.out.println("Thread running");
    }
}

class Program15_RunnableThread {
    public static void main(String[] args) {
        Thread t = new Thread(new MyThread());
        t.start();
    }
}


